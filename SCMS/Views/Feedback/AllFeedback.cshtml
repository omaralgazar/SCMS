@model SCMS.ViewModels.FeedbackListVm

<h2>All Feedbacks</h2>

<table class="table">
    <thead>
        <tr>
            <th>Patient</th>
            <th>Rate</th>
            <th>Feedback</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var f in Model.Items)
        {
            <tr>
                <td>@f.PatientName</td>
                <td>@f.Rate</td>
                <td>@f.FeedbackText</td>
                <td>@f.CreatedAt.ToString("g")</td>
            </tr>
        }
    </tbody>
</table>
